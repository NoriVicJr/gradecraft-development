%th{:style => "text-align: right"}= "#{term_for :groups}"
%th.center{:align => "center", scope: "col", "data-dynatable-no-sort" => "true" }
  - if presenter.grade_checkboxes?
    %span.sr-only Check/Uncheck
    %button.button.select-all{role: "button", type: "button"}= "Check All"
    %button.button.select-none{role: "button", type: "button"}= "Uncheck All"
%th
