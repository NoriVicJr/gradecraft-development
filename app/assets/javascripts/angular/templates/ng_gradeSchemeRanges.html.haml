%span.form_label Low Point Threshold
%input.low-range{:type => 'number','index' => '{{index}}','name' => 'low_points', 'ng-model' => 'element.low_points'}
%span.form_label High Point Threshold
%input.high-range{:type => 'number', 'name' => 'high_points', 'ng-model' => 'element.high_points', 'ng-disabled' => 'index > 0'}
