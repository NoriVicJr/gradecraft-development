= content_nav_for Course, ["Import Grades", assignment_grades_importers_path(@assignment)], @provider_name.capitalize

%h3.pagetitle= "Grade Import from #{@provider_name.capitalize}"

.pageContent
  = render partial: "layouts/alerts"

  = partial_with "importers/assignments_table", locals: { path: assignment_grades_importer_grades_path(@assignment, @provider_name, @course["id"]) } do
    %p= submit_tag "Select grades from assignments to import", class: "button disabled", disabled: true, data: { behavior: "selected-assignments-command" }
