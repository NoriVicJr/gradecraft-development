.has-form{role: "search"}
  %input.students-search-query.search-input{data: { autocompleteUrl: api_students_path }, type: "text", placeholder: "Search #{term_for :students}", "aria-label": "Search #{term_for :students}"}
  %button.search-submit-button.alert{type: "submit", "aria-label": "Search"} Go
